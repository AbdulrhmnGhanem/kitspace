{
  "private": true,
  "workspaces": [
    "packages/gitlab-client",
    "services/frontend",
    "services/partinfo",
    "services/board-files",
    "services/login",
    "services/gitlabql"
  ],
  "scripts": {
    "start:nginx": "./nginx/make_conf && docker-compose up nginx" ,
    "start:gitlab": "docker-compose up gitlab" ,
    "docker": "yarn start:nginx & yarn start:gitlab",
    "start:frontend": "cd services/frontend && yarn start",
    "start:login": "cd services/login && yarn start",
    "start:gitlabql": "cd services/gitlabql && yarn start",
    "serve": "yarn start:frontend & yarn start:login & yarn start:gitlabql",
    "start": "yarn run docker & yarn run serve"
  }
}
